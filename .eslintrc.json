{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "require-jsdoc":"off",
        "valid-jsdoc":"off",
        "no-trailing-spaces":"off",
        "max-len":"off",
        "camelcase":"off",
        "no-unused-vars":"warn"
    }
}
